    "maplibre-gl": "5.1.0"
  },
  "devDependencies": {
    "@types/node": "20.12.8",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "eslint": "8.56.0",
    "lightningcss": "1.24.1",
    "prettier": "3.2.5",
    "typescript": "5.8.2",
    "vite": "6.2.3"
